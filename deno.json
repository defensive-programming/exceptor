{
  "name": "bug-fucker",
  "version": "0.0.50",
  "description": "fuck ur bugs",
  "exports": "./src/index.ts",
  "license": "MIT",
  "tasks": {
    "u": "deno cache --lock-write ./src/index.ts",
    "dev": "deno run -A --watch ./src/playground.ts",
    "build": "deno run -A ./script/build-npm.ts",
    "release": "deno run -A --allow-read npm:release-it@latest --config release.config.js",
    "publish": "deno task publish:npm",
    "launch": "deno task release && deno task build && deno task publish",

    "d": "deno task dev",
    "b": "deno task build",
    "r": "deno task release",
    "p": "deno task publish",
    "l": "deno task launch"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "zod": "npm:zod@^3.23.8",
    "release-it": "npm:release-it@latest",
    "bumper": "npm:@release-it/bumper@latest",
    "~/": "./src/"
  }
}
