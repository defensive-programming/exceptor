{
  "name": "bug-fucker",
  "version": "0.0.50",
  "description": "fuck ur bugs",
  "license": "MIT",
  "tasks": {
    "d": "deno run -A --watch ./src/index.ts",
    "p": "deno run -A --watch ./src/playground.ts",
    "u": "deno cache --lock-write ./src/index.ts",
    "r": "deno run -A --allow-read npm:release-it@latest --config release.config.js",
    "build": "deno run -A ./script/build-npm.ts",
    "publish": "deno task publish:npm",
    "launch": "deno task r && deno task build && deno task publish",
    "bp": "deno task build && deno task publish"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "zod": "npm:zod@^3.23.8",
    "release-it": "npm:release-it@latest",
    "bumper": "npm:@release-it/bumper@latest",
    "~/": "./src/"
  }
}
